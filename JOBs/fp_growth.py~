class TreeNode:
	def __init__(self, name, count, parent):
		self.name = name
		self.count = count
		self.parent = parent
		self.link = None
		self.children = {}
	
	def increment(self, occur):
		self.count += occur

	def display(self, indent = 1):
		print(' '*indent, self.name, ' ', self.name)
		for child in children:
			child.display(indent+1)



























